  "scripts": {
    "serve:web": "nx serve web",
    "serve:api": "nx serve api",
    "build:web": "nx build web --configuration=production",
    "build:api": "nx build api --configuration=production",
    "test": "nx run-many --target=test --all --coverage=true --coverageReporters=lcov --watch=false --browsers=ChromeHeadless && node coverage-merger.js",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "stats": "yarn build:web && webpack-bundle-analyzer dist/apps/web/stats.json",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test --code-coverage --browsers=ChromeHeadless && node coverage-merger.js",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "workspace-schematic": "nx workspace-schematic",
    "clear:nx": "rimraf -rf ./node_modules/.cache/nx",
    "clear:modules": "rimraf -rf ./node_modules",
    "clear:webpack": "rimraf -rf ./node_modules/.cache/webpack",
    "clear:dist": "rimraf -rf ./dist/",
    "graphql:generate": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen graphql-codegen --config codegen.ts",
    "typeorm": "ts-node -O '{\"module\":\"commonjs\"}' -P tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:run": "ts-node -O '{\"module\":\"commonjs\"}' -P tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d libs/core/server/src/lib/database/database.cli.ts",
    "help": "nx help",
    "parser:build": "yarn run ts-jison -t typescript -n PL -o libs/feature/compiler/src/lib/pl.parser.ts libs/feature/compiler/src/lib/pl.jison"
  },
